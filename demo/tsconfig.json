{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*"
      ],
      "ngx-table-editor-dev": [
        "../core/src/index"
      ],
      "ngx-table-editor-dev-extras": [
        "../extras/src/index"
      ]
    },
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
